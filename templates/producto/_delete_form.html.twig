<form class="delete-product-form" action="{{ path('app_producto_delete', {'id': producto.id}) }}" method="post">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ producto.id) }}">
    <button type="button" class="btn btn-danger delete-product-btn" data-id="{{ producto.id }}">Eliminar</button>
</form>
